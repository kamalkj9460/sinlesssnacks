<!-- features-area -->
	<section class="features-area">
		<div class="container">
			<div class="row justify-content-center">
				<div class="col-xl-6 col-lg-8">
					<div class="section-title text-center mb-50">
						<h2 class="title">Some of the best features</h2>
						<p>Most ‘keto’ snacks and supplements are filled with junk. Not ours.</p>
					</div>
				</div>
			</div>
		</div>
		<div class="container custom-container">
			<div class="features-item-wrap">
				<div class="row justify-content-center">
					<div class="col">
						<div class="features-item">
							<div class="features-icon">
								<img src="img/icon/features_icon01.png" alt="">
							</div>
							<div class="features-content">
								<h4 class="title"><a href="#">No Added Sugar</a></h4>
							</div>
						</div>
					</div>
					<div class="col">
						<div class="features-item active">
							<div class="features-icon">
								<img src="img/icon/features_icon02.png" alt="">
							</div>
							<div class="features-content">
								<h4 class="title"><a href="#">Low Carb</a></h4>
							</div>
						</div>
					</div>
					<div class="col">
						<div class="features-item">
							<div class="features-icon">
								<img src="img/icon/features_icon03.png" alt="">
							</div>
							<div class="features-content">
								<h4 class="title"><a href="#">High Protein</a></h4>
							</div>
						</div>
					</div>
					<div class="col">
						<div class="features-item">
							<div class="features-icon">
								<img src="img/icon/features_icon04.png" alt="">
							</div>
							<div class="features-content">
								<h4 class="title"><a href="#">Collagen</a></h4>
							</div>
						</div>
					</div>
					<div class="col">
						<div class="features-item">
							<div class="features-icon">
								<img src="img/icon/features_icon05.png" alt="">
							</div>
							<div class="features-content">
								<h4 class="title"><a href="#">Perfect Taste</a></h4>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
